<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ISampleService" />
            </basicHttpBinding>
            <netTcpBinding>
                <binding name="NetTcpBinding_ISampleService" />
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/SampleService" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_ISampleService" contract="ServiceReference1.ISampleService"
                name="BasicHttpBinding_ISampleService" />
            <endpoint address="net.tcp://localhost:8085/Sample" binding="netTcpBinding"
                bindingConfiguration="NetTcpBinding_ISampleService" contract="ServiceReference1.ISampleService"
                name="NetTcpBinding_ISampleService">
                <identity>
                    <userPrincipalName value="dell-PC\dell" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>